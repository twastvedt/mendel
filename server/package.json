{
  "name": "@mendel/server",
  "version": "1.0.0",
  "description": "Express server for Mendel.",
  "main": "dist/index.js",
  "private": true,
  "dependencies": {
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "tsconfig-paths": "^3.13.0",
    "ts-node": "10.7.0",
    "ts-nameof": "^5.0.0",
    "ttypescript": "^1.5.13",
    "typeorm": "0.2.45",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ts-nameof": "^4.2.1",
    "@types/node": "^16"
  },
  "scripts": {
    "serve": "ts-node-dev -r tsconfig-paths/register --compiler ttypescript src/index.ts",
    "start": "ts-node -r tsconfig-paths/register --transpile-only --compiler ttypescript src/index.ts"
  }
}
