    "vuetify-loader": "^1.7.3",
    "webpack-merge": "^5.8.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "src/client"
