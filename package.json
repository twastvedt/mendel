{
  "name": "garden-data",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "client-build": "vue-cli-service build src/client/main.ts",
    "client-serve": "set NODE_ENV=development&& vue-cli-service serve src/client/main.ts",
    "server-serve": "ts-node-dev -r tsconfig-paths/register --compiler ttypescript src/server/index.ts",
    "start": "concurrently \"npm run client-serve\" \"npm run server-serve\"",
    "typeorm": "typeorm"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@svgdotjs/svg.js": "^3.1.1",
    "@types/d3-geo": "^3.0.2",
